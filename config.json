{
    "project": {
        "name": "Jenkins",
        "description": "A project of jenkins resources.",
        "environment": "Development",
        "purpose": "Operational / Developer tooling"
    },
    "vpc": {
        "name": "jenkins-vpc",
        "ipRange": "10.10.10.0/24",
        "region": "sgp1"
    },
    "sshkey": {
        "name": "jenkins-key"
    },
    "tagMaster": {
        "name": "jenkins-master"
    },
    "tagSlave": {
        "name": "jenkins-slave"
    },
    "firewallMaster": {
        "name": "jenkins-master-firewall"
    },
    "volumeMaster": {
        "name": "jenkins-master-home",
        "region": "sgp1",
        "size": 50,
        "initialFilesystemType": "xfs"
    },
    "dropletMaster": {
        "name": "jenkins-master",
        "image": "docker-20-04",
        "region": "sgp1",
        "size": "s-1vcpu-1gb"
    },
    "firewallSlave": {
        "name": "jenkins-slave-firewall"
    },
    "volumeSlave": {
        "name": "jenkins-slave-home",
        "region": "sgp1",
        "size": 50,
        "initialFilesystemType": "xfs"
    },
    "dropletSlave": {
        "name": "jenkins-slave",
        "image": "docker-20-04",
        "region": "sgp1",
        "size": "s-1vcpu-1gb"
    },
    "misc": {
        "masterImage": "jenkins/jenkins:alpine",
        "slaveImage": "jenkins/jnlp-slave:alpine",
        "jenkinsUser": "admin",
        "jenkinsPassword": "admin",
        "dobsMasterToken": "put-your-do-token-here",
        "dobsSlaveToken": "put-your-do-token-here"
    }
}
